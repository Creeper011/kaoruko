discord:
  prefix: "?ka"
  owner_id: discord owner id
  intents:
    guilds: true
    messages: true
    message_content: true
  is_mobile: true

download:
  file_size_limit: 26214400  # 25MB
  blacklist_sites:
    - "example.com"
    - "anotherexample.com"

drive:
  credentials_path: "/path/to/credentials.json"
  folder_id: "your_google_drive_folder_id"

redis:
  host: "localhost"
  port: 6379
  cache_db: 0
  login_db: 1
  username: null # Set to null if no username is required
  password: null # Set to null if no password is required